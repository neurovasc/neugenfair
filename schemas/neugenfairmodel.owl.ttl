@prefix faldo: <http://biohackathon.org/resource/faldo#> .
@prefix geno: <http://purl.obolibrary.org/obo/GENO_> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/SIO_> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix so: <http://purl.obolibrary.org/obo/SO_> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/neugenfair/schema.owl.ttl> a owl:Ontology ;
    rdfs:label "neugenfairSchema" ;
    skos:definition """neugenfair schema data model for representing phenoclinical genomic data in the ICAN dataset
Schema for the neugenfair tool. Defines classes and attributes, supporting FAIR transformation of genomic and clinical data into Linked Data.

Schema for the AssemblySequence describing the chromosome or sequence that is the reference for the Region of the SequenceAlteration.

Schema for the (genomic) Position describing the start or the end of the Region where the SequenceAlteration is located.

Schema for the neugenfair tool. Defines classes and attributes, supporting FAIR transformation of genomic and clinical data into Linked Data.

Region class schema for the neugenfair tool. Defines Region where the SeqeunceAlteration occured as defined in FALDO ontology.

SequenceAlteration class schema for the neugenfair tool. Defines SequenceAlteration and the relations to other adjacent concepts.

VariantIdentifier class. The identifiers can following several nomenclatures or database convention naming (HGVSid, dbsnp, rsid). On SequenceAlteration can have several identifiers. One identifier can refer to several SequenceAlterations (complex alterations e.g. copy number variation).
""" .

<https://w3id.org/neugenfair/schema/AlternateAllele> a owl:Class ;
    rdfs:label "AlternateAllele" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ] ;
    skos:definition "Represents the alternate allele (geno:0000002)." ;
    skos:exactMatch geno:0000002 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/ReferenceAllele> a owl:Class ;
    rdfs:label "ReferenceAllele" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/value> ] ;
    skos:definition "Represents the reference allele (geno:0000036)." ;
    skos:exactMatch geno:0000036 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/:Nomenclature#COSMIC> a owl:Class ;
    rdfs:label "COSMIC" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#ClinVar> a owl:Class ;
    rdfs:label "ClinVar" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#Ensembl> a owl:Class ;
    rdfs:label "Ensembl" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#HGVS> a owl:Class ;
    rdfs:label "HGVS" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#HUGO> a owl:Class ;
    rdfs:label "HUGO" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#Other> a owl:Class ;
    rdfs:label "Other" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#dbSNP> a owl:Class ;
    rdfs:label "dbSNP" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/:Nomenclature#rsID> a owl:Class ;
    rdfs:label "rsID" ;
    rdfs:subClassOf <https://w3id.org/neugenfair/schema/:Nomenclature> .

<https://w3id.org/neugenfair/schema/VariantIdentifier> a owl:Class ;
    rdfs:label "VariantIdentifier" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/neugenfair/schema/:Nomenclature> ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_source> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_source> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_source> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ] ;
    skos:definition "A unique identifier for a sequence alteration within a database or nomenclature." ;
    skos:exactMatch sio:000675 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/begins_at> a owl:ObjectProperty ;
    rdfs:label "begins_at" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/Region> ;
    rdfs:range <https://w3id.org/neugenfair/schema/Position> ;
    skos:definition "The beginning of the location of the sequence alteration." ;
    skos:exactMatch faldo:begin ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/ends_at> a owl:ObjectProperty ;
    rdfs:label "ends_at" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/Region> ;
    rdfs:range <https://w3id.org/neugenfair/schema/Position> ;
    skos:definition "The end of the location of the sequence alteration." ;
    skos:exactMatch faldo:end ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/has_alternate_part> a owl:ObjectProperty ;
    rdfs:label "has_alternate_part" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/SequenceAlteration> ;
    rdfs:range <https://w3id.org/neugenfair/schema/AlternateAllele> ;
    skos:definition "Links the SequenceAlteration to its AlternateAllele." ;
    skos:exactMatch geno:0000382 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/has_identifier> a owl:ObjectProperty ;
    rdfs:label "has_identifier" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/SequenceAlteration> ;
    rdfs:range <https://w3id.org/neugenfair/schema/VariantIdentifier> ;
    skos:definition "Relation between a SequenceAlteration and its VariantIdentifier" ;
    skos:inScheme <https://w3id.org/neugenfair/schema> ;
    skos:mappingRelation sio:0000671 .

<https://w3id.org/neugenfair/schema/has_reference> a owl:ObjectProperty ;
    rdfs:label "has_reference" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/Region> ;
    rdfs:range <https://w3id.org/neugenfair/schema/AssemblySequence> ;
    skos:definition "The reference sequence (contig, sequence, chromosome)." ;
    skos:exactMatch faldo:reference ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/has_reference_part> a owl:ObjectProperty ;
    rdfs:label "has_reference_part" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/SequenceAlteration> ;
    rdfs:range <https://w3id.org/neugenfair/schema/ReferenceAllele> ;
    skos:definition "Links the SequenceAlteration to its ReferenceAllele." ;
    skos:exactMatch geno:0000385 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/has_source> a owl:ObjectProperty ;
    rdfs:label "has_source" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/VariantIdentifier> ;
    rdfs:range <https://w3id.org/neugenfair/schema/:Nomenclature> ;
    skos:definition "The nomenclature or database of the identifier (e.g. HGVS, dbSNP)." ;
    skos:inScheme <https://w3id.org/neugenfair/schema> ;
    skos:mappingRelation sio:000253 .

<https://w3id.org/neugenfair/schema/label> a owl:DatatypeProperty ;
    rdfs:label "label" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/AssemblySequence> ;
    rdfs:range xsd:string ;
    skos:definition "A human-readable relatable name for the sequence (e.g. chr1)." ;
    skos:exactMatch rdfs:label ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/location> a owl:ObjectProperty ;
    rdfs:label "location" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/SequenceAlteration> ;
    rdfs:range <https://w3id.org/neugenfair/schema/Region> ;
    skos:definition "Links the SequenceAlteration to its (genomic) Region." ;
    skos:exactMatch faldo:location ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/position> a owl:DatatypeProperty ;
    rdfs:label "position" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/Position> ;
    rdfs:range xsd:integer ;
    skos:definition "Exact (genomic) Position of a SequenceAlteration." ;
    skos:exactMatch geno:0000903 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/same_as> a owl:DatatypeProperty ;
    rdfs:label "same_as" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/AssemblySequence> ;
    rdfs:range xsd:string ;
    skos:definition "Link to an external reference for the sequence (e.g. a URI from a genome browser or database)." ;
    skos:exactMatch owl:sameAs ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/AssemblySequence> a owl:Class ;
    rdfs:label "AssemblySequence" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/same_as> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/label> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_value> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/same_as> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/neugenfair/schema/label> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/label> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/same_as> ] ;
    skos:definition "AssemblySequence is a chromosome or sequence in a reference genome." ;
    skos:exactMatch geno:0000960,
        so:0000353 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> ;
    skos:relatedMatch <go:0005694>,
        sio:000899 .

<https://w3id.org/neugenfair/schema/Region> a owl:Class ;
    rdfs:label "Region" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/begins_at> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/begins_at> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/neugenfair/schema/Position> ;
            owl:onProperty <https://w3id.org/neugenfair/schema/ends_at> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/ends_at> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/neugenfair/schema/Position> ;
            owl:onProperty <https://w3id.org/neugenfair/schema/begins_at> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/neugenfair/schema/AssemblySequence> ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/ends_at> ] ;
    skos:definition "(genomic) Region of a SequenceAlteration." ;
    skos:exactMatch faldo:Region ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/SequenceAlteration> a owl:Class ;
    rdfs:label "SequenceAlteration" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_alternate_part> ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty <https://w3id.org/neugenfair/schema/location> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/location> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/location> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference_part> ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_identifier> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_identifier> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_alternate_part> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_identifier> ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_alternate_part> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference_part> ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty <https://w3id.org/neugenfair/schema/has_reference_part> ] ;
    skos:definition "A representation of a SequenceAlteration." ;
    skos:exactMatch geno:0000660,
        so:0001059 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/Position> a owl:Class ;
    rdfs:label "Position" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty <https://w3id.org/neugenfair/schema/position> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/position> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/neugenfair/schema/position> ] ;
    skos:closeMatch so:0000735 ;
    skos:definition "(genomic) Position." ;
    skos:exactMatch faldo:ExactPosition ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/has_value> a owl:DatatypeProperty ;
    rdfs:label "has_value" ;
    rdfs:domain <https://w3id.org/neugenfair/schema/VariantIdentifier> ;
    rdfs:range xsd:string ;
    skos:definition "Sequence name",
        "The value of the identifier." ;
    skos:exactMatch sio:000300 ;
    skos:inScheme <https://w3id.org/neugenfair/schema> ;
    skos:mappingRelation sio:000300 .

<https://w3id.org/neugenfair/schema/value> a owl:DatatypeProperty ;
    rdfs:label "value" ;
    skos:definition "The value of the alternate allele.",
        "The value of the reference allele." ;
    skos:inScheme <https://w3id.org/neugenfair/schema> .

<https://w3id.org/neugenfair/schema/:Nomenclature> a owl:Class ;
    owl:unionOf ( <https://w3id.org/neugenfair/schema/:Nomenclature#HGVS> <https://w3id.org/neugenfair/schema/:Nomenclature#dbSNP> <https://w3id.org/neugenfair/schema/:Nomenclature#rsID> <https://w3id.org/neugenfair/schema/:Nomenclature#ClinVar> <https://w3id.org/neugenfair/schema/:Nomenclature#COSMIC> <https://w3id.org/neugenfair/schema/:Nomenclature#Ensembl> <https://w3id.org/neugenfair/schema/:Nomenclature#HUGO> <https://w3id.org/neugenfair/schema/:Nomenclature#Other> ) ;
    linkml:permissible_values <https://w3id.org/neugenfair/schema/:Nomenclature#COSMIC>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#ClinVar>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#Ensembl>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#HGVS>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#HUGO>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#Other>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#dbSNP>,
        <https://w3id.org/neugenfair/schema/:Nomenclature#rsID> .


